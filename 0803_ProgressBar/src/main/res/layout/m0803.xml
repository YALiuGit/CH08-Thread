<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m0803_pro"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:orientation="vertical"
    android:padding="@dimen/marginsize"
    tools:context=".M0803">

    <TextView
        android:id="@+id/m0803_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/Teal"
        android:text="@string/m0803_t001"
        android:textAllCaps="true"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p001"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="30"
        android:progressTint="@color/Navy"
        android:secondaryProgress="60"
        android:secondaryProgressTint="#F63939" />

    <TextView
        android:id="@+id/m0803_t002"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@color/Yellow"
        android:text="@string/m0803_t002"
        android:textAllCaps="true"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p002"
        style="@android:style/Widget.ProgressBar.Inverse"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/Yellow"
        android:indeterminate="false"
        android:max="100"
        android:progress="30" />

    <TextView
        android:id="@+id/m0803_t003"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@color/Teal"
        android:text="@string/m0803_t003"
        android:textAllCaps="true"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p003"
        style="@android:style/Widget.ProgressBar.Large"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/Teal"
        android:max="100"
        android:progress="30"
        android:progressTint="@color/Blue" />

    <TextView
        android:id="@+id/m0803_t004"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@color/Yellow"
        android:text="@string/m0803_t004"
        android:textAllCaps="true"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p004"
        style="@android:style/Widget.ProgressBar.Small"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/Yellow" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pa01"
        android:gravity="end"
        android:text="@string/myname"
        android:textColor="@color/Blue"
        android:textSize="@dimen/fontsize" />
</LinearLayout>